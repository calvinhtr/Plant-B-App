{
  "expo": {
    "name": "PlantB",
    "slug": "PlantB",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/frog_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/frog_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/frog_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.PlantB"
    },
    "web": {
      "favicon": "./assets/iamges/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "54c31d2f-b721-44e9-b1ed-403a2c52fa12"
      }
    }
  }
}
